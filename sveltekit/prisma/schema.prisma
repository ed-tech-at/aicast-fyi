generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}


model ApiKeys {
  apiId           Int      @id @default(autoincrement())
  apiType         String?
  apiKey          String?   @db.VarChar(255)
  apiPlaceholder  String?   @db.VarChar(255)
}

model Episodes {
  id           String   @id @default(uuid())
  episodeUrl   String?   @unique @db.VarChar(255)
  email        String
  title        String?
  desc         String?
  apiKeyLLM    String?
  apiKeyAudio  String?
  createdDate  DateTime @default(now())
  updatedDate  DateTime?
}

